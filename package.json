{
  "name": "lib-rs",
  "version": "0.0.0",
  "description": "Some reusable Rust libraries for my personal projects.",
  "author": "Gabriel Putra Pinastika <gabri.putra@gmail.com>",
  "repository": {
    "type": "github",
    "url": "https://github.com/0x67/lib-rs"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "git-cz",
